page {
  font-family: 'Times New Roman', Times, serif;
  background: #dedede;
  width: 100%;
  height: 100%;
}

.box1 {
  width: 100%;
  height: calc(100% - 762rpx);
  background: #686868;
  position: relative;
}

.box1_text {
  color: #fff;
  position: absolute;
  bottom: 0;
  right: 30rpx;
  font-size: 60px;
}
.box1 .calculate_area{
      font-size: 60px;
      color: white;
      
}


.box2 {
  width: 100%;
}

.box2_list {
  width: 100%;
  border-bottom: 1px solid #fff;
  float: left;
}

.box2 ul li {
  display: block;
  width: 25%;
  height: 150rpx;
  line-height: 150rpx;
  background: #efefef;
  float: left;
  color: #766464;
  border-left: 1px solid #fff;
  margin-left: -1px;
  font-size: 30px;
  text-align: center;
}

.box2 ul .first_row_color {
  background: #ebc2c5;
}

.box2 ul .NumberStyle {
  background: #ffa47a;
  color: #fff;
  font-size: 60px;
}

.box2 ul .w50 {
  width: 50%;
}

.border {
  border: 0px;
}

.goback {
  width: 25%;
  height: 150rpx;
}
