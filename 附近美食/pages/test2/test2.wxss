page{
    width: 100% ;
    height: 100%;
}
.search{
    width: 100%;
    height: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.search input{
  width:70%;
  height:80%;
  border: 1px solid gray;
  border-right: none;
}
.search icon{
    height:80%;
    width: 60rpx;
    border: 1px solid gray;
    border-left: none;
    display: flex;
    align-items: center;
}